<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="ItoSoftware\Base\ModelBundle\Entity\PlanRepository" name="ItoSoftware\Base\ModelBundle\Entity\Plan">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="tittle" type="string" column="tittle" length="255"/>
    <field name="date" type="datetime" column="date"/>
    <field name="registrarionDate" type="datetime" column="registrarion_date"/>
    <field name="active" type="smallint" column="active"/>

  </entity>
</doctrine-mapping>
